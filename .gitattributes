# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

#git-lfs setting
*.bdr filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.cdk filter=lfs diff=lfs merge=lfs -text
*.csv filter=lfs diff=lfs merge=lfs -text
*.dat filter=lfs diff=lfs merge=lfs -text
*.dsk filter=lfs diff=lfs merge=lfs -text
*.h5 filter=lfs diff=lfs merge=lfs -text
*.mtx filter=lfs diff=lfs merge=lfs -text
*.net filter=lfs diff=lfs merge=lfs -text
*.omx filter=lfs diff=lfs merge=lfs -text
*.pk filter=lfs diff=lfs merge=lfs -text
*.tnw filter=lfs diff=lfs merge=lfs -text
*.tsv filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.BX filter=lfs diff=lfs merge=lfs -text
*.DBF filter=lfs diff=lfs merge=lfs -text
*.DCB filter=lfs diff=lfs merge=lfs -text
*.MDX filter=lfs diff=lfs merge=lfs -text  
